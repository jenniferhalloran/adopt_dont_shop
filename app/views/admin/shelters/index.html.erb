<div id = "all-shelters">
  <h2> All Shelters: </h2>
  <% @shelters.each do |shelter| %>
    <p> <%= shelter.name %> </p>
  <% end %>
</div>
<hr>

<div id = "pending-shelters">
  <h3>Shelters with Pending Applications: </h3>
  <% @pending.each do |shelter| %>
  <%= shelter.name %>
  <% end %>
</div>
